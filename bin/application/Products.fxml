<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ProductController">
   <children>
      <AnchorPane layoutX="21.0" layoutY="33.0" prefHeight="497.0" prefWidth="200.0" styleClass="white-bg" stylesheets="@dashboard.css">
         <children>
            <Label layoutX="9.0" layoutY="61.0" text="Product  ID :" />
            <TextField fx:id="PetId" layoutX="114.0" layoutY="56.0" prefHeight="31.0" prefWidth="118.0" />
            <Label layoutX="2.0" layoutY="121.0" text="Product  Name :" />
            <TextField fx:id="PetName" layoutX="125.0" layoutY="116.0" prefHeight="31.0" prefWidth="118.0" />
            <Label layoutX="6.0" layoutY="179.0" text="Product  raice :" />
            <TextField fx:id="PetRaice" layoutX="125.0" layoutY="174.0" prefHeight="31.0" prefWidth="118.0" />
            <Label layoutX="8.0" layoutY="231.0" text="Product  Age :" />
            <TextField fx:id="PetAge" layoutX="125.0" layoutY="226.0" prefHeight="31.0" prefWidth="118.0" />
            <Button fx:id="AddBtn" layoutX="38.0" layoutY="373.0" mnemonicParsing="false" onAction="#AddPet" text="Add" />
            <Button fx:id="UpdateBtn" layoutX="125.0" layoutY="373.0" mnemonicParsing="false" onAction="#UpdatePet" text="Update" />
            <Button fx:id="DeleteBtn" layoutX="82.0" layoutY="436.0" mnemonicParsing="false" onAction="#DeletePet" text="Delete" />
            <Label layoutX="5.0" layoutY="280.0" text="Product  Price :" />
            <TextField fx:id="PetPrice" layoutX="125.0" layoutY="275.0" prefHeight="31.0" prefWidth="118.0" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="274.0" layoutY="33.0" prefHeight="497.0" prefWidth="594.0" styleClass="white-bg" stylesheets="@dashboard.css">
         <children>
            <TextField fx:id="searchLabel" layoutX="41.0" layoutY="36.0" promptText="Search" />
            <TableView fx:id="TablePets" layoutX="19.0" layoutY="88.0" prefHeight="392.0" prefWidth="561.0">
               <columns>
                  <TableColumn fx:id="TPetId" prefWidth="94.0" text="Product Id" />
                  <TableColumn fx:id="TPetName" prefWidth="118.0" text="Product  Name" />
                  <TableColumn fx:id="TPetRace" prefWidth="124.0" text="Product  raice" />
                  <TableColumn fx:id="TPetAge" prefWidth="112.0" text="Product  age" />
                  <TableColumn fx:id="TPetPrice" prefWidth="117.0" text="Product  price" />
               </columns>
            </TableView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
